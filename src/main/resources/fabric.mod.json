{
  "schemaVersion": 1,
  "id": "architects_palette",
  "version": "${version}",
  "name": "Architect's Palette Fabric",
  "description": "A mod that adds new stylish and unique building blocks. This is a port of Jsburg's mod",
  "authors": [
    "Jsburg (author)",
    "Slomaxonical (Fabric port)",
    "unilock (1.20.1 update)",
    "WerySkok (1.19.4 backport)"
  ],
  "contact": {
    "sources": "https://github.com/WerySkok/Architects-Palette-Fabric"
  },
  "license": "LGPL-3.0-only",
  "icon": "assets/architects_palette/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.slomaxonical.architectspalette.ArchitectsPalette"
    ],
    "client": [
      "com.slomaxonical.architectspalette.ArchitectsPaletteClient"
    ],
    "rei_client": [
      "com.slomaxonical.architectspalette.compat.rei.APPlugin"
    ],
    "fabric-datagen": [
      "com.slomaxonical.architectspalette.datagen.APDatagen"
    ],
    "emi": [
      "com.slomaxonical.architectspalette.compat.emi.APEmiPlugin"
    ]
  },
  "mixins": [
    "architectspalette.mixins.json"
  ],
  "depends": {
    "fabricloader": "*",
    "minecraft": "1.19.4",
    "fabric-api": "*",
    "owo": "*"
  },
  "accessWidener": "architectspalette.accesswidener"
}
